<%= form_for :admin_login, url: api_v1_categories_path do|f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :phone_number %><br>
    <%= f.text_field :ph_no %>
  </p>

  <p>
    <%= f.label :security %><br>
    <%= f.text_field :pass %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>